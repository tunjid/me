find:
SELECT *
FROM userEntity
WHERE id = :id;

insertOrIgnore:
INSERT OR IGNORE INTO userEntity
VALUES ?;

upsert:
INSERT INTO userEntity
VALUES ?
ON CONFLICT(id) DO UPDATE SET
    first_name=excluded.first_name,
    last_name=excluded.last_name,
    full_name=excluded.full_name,
    image_url=excluded.image_url;